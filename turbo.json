{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "@clhs-scoreboard/texas#build": {
      "dependsOn": ["^build", "$NEXT_PUBLIC_GA_ID", "$NEXT_PUBLIC_VERCEL_URl"],
      "outputs": [".next/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
